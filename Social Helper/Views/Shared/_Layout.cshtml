                        <a class="navbar-brand" href="/">Social Helper</a>
                        <a href="/"><span>EN</span></a>
                    </div>

                    <div id="navbar" class="navbar-collapse collapse">
                        <ul class="nav navbar-nav navbar-right">
                            <li class="active"><a href="#" data-nav-section="home"><span>Главная</span></a></li>
                            <li><a href="#" data-nav-section="services"><span>Возможности</span></a></li>
                            <li><a href="#" data-nav-section="blog"><span>Уроки</span></a></li>
                        </ul>
                    </div>
                    }
                    else
                    {
                    <div class="navbar-header">
